--- 
import { EXPERIENCIA_DATA } from "../data/experiencia";
import Experiencia from "./Experiencia.astro";

--- 
<div id="experiencia-laboral">
    <h2 class="titulo">Experiencia Laboral</h2>
    <section>
        {EXPERIENCIA_DATA.map(item =>(
           <Experiencia
           titulo={item.titulo}
           tecnologias={item.tecnologias}
           descripcion={item.descripcion}
           tiempo={item.tiempo}
           /> 
        ))}
    </section>
</div>